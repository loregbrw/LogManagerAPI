<?xml version="1.0" encoding="utf-8"?>
<root>
    <xsd:schema id="root"
        xmlns=""
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
        <xsd:import namespace="http://www.w3.org/XML/1998/namespace" />
        <xsd:element name="root"
            msdata:IsDataSet="true">
            <xsd:complexType>
                <xsd:choice maxOccurs="unbounded">
                    <xsd:element name="data">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="value"
                                    type="xsd:string"
                                    minOccurs="0" />
                                <xsd:element name="comment"
                                    type="xsd:string"
                                    minOccurs="0" />
                            </xsd:sequence>
                            <xsd:attribute name="name"
                                use="required"
                                type="xsd:string" />
                            <xsd:attribute name="xml:space"
                                type="xsd:string" />
                        </xsd:complexType>
                    </xsd:element>
                </xsd:choice>
            </xsd:complexType>
        </xsd:element>
    </xsd:schema>

    <!-- SERVER ERROR -->

    <data name="UnknownError"
        xml:space="preserve">
        <value>Unknown internal server error.</value>
    </data>
    <data name="UnknownErrorMapping"
        xml:space="preserve">
        <value>Unknown error mapping entity.</value>
    </data>

    <!-- NOT FOUND -->

    <data name="EntityNotFound"
        xml:space="preserve">
        <value>{0} not found.</value>
    </data>

    <!-- BAD REQUEST -->

    <data name="NullOrWhiteSpace"
        xml:space="preserve">
        <value>{0} cannot be null or white space.</value>
    </data>
    <data name="FieldTooLong"
        xml:space="preserve">
        <value>{0} cannot longer than {1} characters.</value>
    </data>
    <data name="FieldRequired"
        xml:space="preserve">
        <value>{0} is required.</value>
    </data>
    <data name="InvalidEnumValue"
        xml:space="preserve">
        <value>{1} isn't a valid value for {0}.</value>
    </data>

    <data name="InvalidPassword"
        xml:space="preserve">
        <value>Invalid password: minimum 8 characters, 1 number, 1 letter, and 1 special character.</value>
    </data>

    <!-- AUTH -->

    <data name="WrongPassword"
        xml:space="preserve">
        <value>Incorrect password.</value>
    </data>

    <data name="NotAuthenticated"
        xml:space="preserve">
        <value>User not authenticated.</value>
    </data>

    <data name="NoRegisteredPassword"
        xml:space="preserve">
        <value>User hasn't registered a password yet.</value>
    </data>

    <data name="NoAccess"
        xml:space="preserve">
        <value>User doesn't have access.</value>
    </data>

    <!-- CONFLICT -->

    <data name="AlreadyExists"
        xml:space="preserve">
        <value>{0} already exists on the database.</value>
    </data>


    <data name="UserCodeAlreadyExists"
        xml:space="preserve">
        <value>A user with the code {0} already exists.</value>
    </data>

    <data name="UserEmailAlreadyExists"
        xml:space="preserve">
        <value>A user with the email {0} already exists.</value>
    </data>

    <data name="UserAlreadyRegistered"
        xml:space="preserve">
        <value>User {0} is already registered.</value>
    </data>

    <data name="MissingRegistrationEmail"
        xml:space="preserve">
        <value>It's not possible to complete the registration of a user without a registered email.</value>
    </data>

    <!-- CSV -->

    <data name="EmptyCsv"
        xml:space="preserve">
        <value>Csv is empty.</value>
    </data>
    <data name="MissingFieldCsv"
        xml:space="preserve">
        <value>Missing field at position {0}.</value>
    </data>
    <data name="BadDataCsv"
        xml:space="preserve">
        <value>Bad data found at row {0}.</value>
    </data>
    <data name="InvalidHeaderCsv"
        xml:space="preserve">
        <value>Invalid csv header.</value>
    </data>
    <data name="ErrorProcessingCsv"
        xml:space="preserve">
        <value>Error processing csv: {0}.</value>
    </data>
    <data name="MissingCsv"
        xml:space="preserve">
        <value>Csv is missing.</value>
    </data>
    <data name="MissingHeader"
        xml:space="preserve">
        <value>Csv header is missing.</value>
    </data>
    <data name="DuplicateHeaders"
        xml:space="preserve">
        <value>Duplicate headers for property {0} found in CSV.</value>
    </data>
    <data name="NoMatchingHeaders"
        xml:space="preserve">
        <value>No matching headers found in CSV.</value>
    </data>
    <data name="InvalidCurrencyFormat"
        xml:space="preserve">
        <value>Invalid currency format for {0}.</value>
    </data>
    <data name="InvalidCurrencyType"
        xml:space="preserve">
        <value>Invalid type {0} for currency conversion. Expected decimal or numeric type.</value>
    </data>
    <data name="InvalidDelimiter"
        xml:space="preserve">
        <value>Invalid CSV delimiter. Allowed delimiters are: ; | , and tab.</value>
    </data>


    <!-- TOKEN -->

    <data name="MissingClaim"
        xml:space="preserve">
        <value>Token missing {0} claim.</value>
    </data>
    <data name="InvalidClaim"
        xml:space="preserve">
        <value>Invalid claim {0}.</value>
    </data>
    <data name="TokenExpired"
        xml:space="preserve">
        <value>Token expired.</value>
    </data>
    <data name="InvalidToken"
        xml:space="preserve">
        <value>Invalid token.</value>
    </data>
    <data name="TokenValidationFailed"
        xml:space="preserve">
        <value>Token validation failed.</value>
    </data>
    <data name="MissingAuthorizationHeader"
        xml:space="preserve">
        <value>Missing or invalid Authorization header.</value>
    </data>
    <data name="ForbiddenEndpoint"
        xml:space="preserve">
        <value>You do not have permission to access this endpoint.</value>
    </data>

    <!-- USER -->

    <data name="UserMissingEmail"
        xml:space="preserve">
        <value>User doesn't have a registered email.</value>
    </data>
</root>